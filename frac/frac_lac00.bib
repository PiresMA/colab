
@article{ WOS:A1996VV82000001,
Author = {Smith, TG and Lange, GD and Marks, WB},
Title = {Fractal methods and results in cellular morphology - Dimensions,
   lacunarity and multifractals},
Journal = {JOURNAL OF NEUROSCIENCE METHODS},
